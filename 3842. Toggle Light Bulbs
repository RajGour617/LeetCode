class Solution {
    public List<Integer> toggleLightBulbs(List<Integer> bulbs) {
        boolean[] state = new boolean[101]; // index 1 to 100

        for (int bulb : bulbs) {
            state[bulb] = !state[bulb]; // toggle
        }

        List<Integer> result = new ArrayList<>();

        for (int i = 1; i <= 100; i++) {
            if (state[i]) {
                result.add(i);
            }
        }

        return result;
    }
}
